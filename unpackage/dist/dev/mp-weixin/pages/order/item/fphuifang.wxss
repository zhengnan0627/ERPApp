
page{
		background: #EEEEEE;
}
.container {
		width: 100vw;
		overflow: hidden;
}
.content {
		width: 100vw;
		height: 200px;
		margin-top: 10px;
		background-color: #EEEEEE;
}
.tabcontrol{
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		height: 40px;	
		line-height: 40px;
		width: 750rpx;
	    text-align: center;
	    font-size: 13px;
	    background-color: #fff;
	    z-index: 9;
		border-bottom: 2px solid #EEEEEE;
}
.tabcontrolitem{
		-webkit-box-flex: 1;
		-webkit-flex: 1;
		        flex: 1;
		height: 40px;
		line-height: 40px;
}
.tabcontrolitem2 {
		height: 40px;
		line-height: 40px;
}
.active{
		color: #00aa00;
		height: 34px !important;
		margin-top: 3px;
		line-height: 34px;
		border-bottom-width: 3px ;
		border-bottom-style: solid;
		border-bottom-color: #00aa00;
}

