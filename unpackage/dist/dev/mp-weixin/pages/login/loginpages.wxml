<view class="container"><view class="navbar">{{'登录:'+name[current]+''}}</view><view style="width:100vw;height:45px;"></view><view class="swiper"><swiper class="swiper_warp" style="height:100%;" interval="2000" current="{{current}}" duration="20" data-event-opts="{{[['change',[['currentchange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{swiperlist}}" wx:for-item="item" wx:for-index="i" wx:key="i"><block><swiper-item><view class="swiper-item"><image src="{{item.swiperurl}}" mode></image></view></swiper-item></block></block></swiper></view><view class="input"><view class="input-item"><uni-icons style="margin-left:5px;" vue-id="201497ec-1" type="contact" size="30" color="#00aa00" bind:__l="__l"></uni-icons><input class="content" style="border-bottom:2px solid #EEEEEE;margin-right:2px;" type="number" value="{{user.loginname}}" cursor="1" maxlength="20" placeholder="请输入手机号" data-event-opts="{{[['input',[['__set_model',['$0','loginname','$event',[]],['user']]]]]}}" bindinput="__e"/></view><view class="input-item"><uni-icons style="margin-left:5px;" vue-id="201497ec-2" type="locked" size="30" color="#00aa00" bind:__l="__l"></uni-icons><input class="content" type="number" value="{{user.password}}" placeholder="请输入登陆密码" maxlength="20" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['user']]]]]}}" bindinput="__e"/></view></view><view class="footer"><view data-event-opts="{{[['tap',[['passwordTo',['$event']]]]]}}" class="text" bindtap="__e">忘记密码</view><view class="text" style="display:flex;"><checkbox-group data-event-opts="{{[['change',[['change',['$event']]]]]}}" bindchange="__e"><label><checkbox value="cb" checked="{{user.checked}}"></checkbox></label><label style="display:none;"><checkbox value="cb"></checkbox>未选中</label></checkbox-group>记住密码</view></view><view data-event-opts="{{[['tap',[['login',['$event']]]]]}}" class="login" bindtap="__e">登录</view></view>