
.container {
	 width: 100vw;
	 height: 100vh;
	 background-color: #FFFFFF;
	 text-align: center;
	 overflow: hidden;
}
.icon-img {
	 width: 28vw;
	 height: 15vh;
	 margin: 70px auto;
}
.icon-img image {
	 width: 100%;
	 height: 100%;
}
.item {
	 width: 80vw;
	 height: 50px;
	 line-height: 50px;
	 font-size: 24px;
	 margin: 30px auto;
	 background-color: #00a200;
	 color: #fff7f7;
	 border-radius: 7px;
}
.footer {
	 position: fixed;
	 bottom: 0;
	 left: 20vw;
	 color: #757575;
}


