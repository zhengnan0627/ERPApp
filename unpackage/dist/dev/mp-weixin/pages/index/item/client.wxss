
.container{
	width: 100vw;
	height: 100vh;
	background-color: #FFFFFF;
}
.content {
	width: 100vw;
	height: 100vh;
}
.content-item{
	width: 100vw;
	height: 80px;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	border-bottom: 2px solid #EEEEEE;
}
.item-left {
	width: 15vw;
	height: 80px;
	line-height: 80px;
	font-size: 22px;
	text-align: center;
}
.item-middle {
	width: 70vw;
	height: 80px;
	line-height: 40px;
	font-size: 14px;
}
.item-right {
	height: 80px;
	line-height: 40px;
	font-size: 17px;
	text-align: right;
	margin-right: 5px;
}

