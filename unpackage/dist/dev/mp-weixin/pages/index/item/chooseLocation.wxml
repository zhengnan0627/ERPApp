<view><view data-ref="mapTop" class="tool-bar-fixed vue-ref"><view class="search_view"><image class="search_icon" src="../../../static/image/back.png" data-event-opts="{{[['tap',[['back',['$event']]]]]}}" bindtap="__e"></image><view class="search_input">地址纠正</view></view></view><map class="vue-ref" style="{{(mapStyle)}}" id="map" markers="{{markers}}" controls="{{controls}}" longitude="{{location.longitude}}" latitude="{{location.latitude}}" data-ref="map" data-event-opts="{{[['regionchange',[['regionchange',['$event']]]]]}}" bindregionchange="__e"><image class="cover_marker" style="{{'left:'+(mapWAH.width/2-10+'px')+';'+('top:'+(mapWAH.height/2-30+'px')+';')}}" src="../../../static/image/chooselocation.png"></image><image class="do_get_location" src="../../../static/image/correct.png" data-event-opts="{{[['tap',[['doCheckPermission',['$event']]]]]}}" bindtap="__e"></image></map><view data-ref="mapBottom" class="main_bottom vue-ref"><view class="old_address"><text class="add_text">原地址:清镇市新店镇街上</text></view><view class="choose_address"><text class="add_text">{{"当前地址:"+addressText}}</text></view><text data-event-opts="{{[['tap',[['emitcorrect',['$event']]]]]}}" class="uni-btn-v" bindtap="__e">选择这里</text></view><block wx:if="{{showListView}}"><view class="search_list_view"><block wx:for="{{addressList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><view data-event-opts="{{[['tap',[['chooseIs',[index,'$0'],[[['addressList','id',item.id]]]]]]]}}" class="addres_item" bindtap="__e"><text class="text_name">{{item.name}}</text><view class="_br"></view><text class="text_address">{{item.district+item.address}}</text><block wx:if="{{addressIndex===index}}"><image class="choose_icon" src="../../../static/image/correct.png"></image></block></view></block></view></block></view>