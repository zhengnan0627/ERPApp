
.container{
	width: 100vw;
	background-color: #FFFFFF;
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
}
.content {
	width: 100vw;
}
.content-item{
	width: 100vw;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	border-bottom: 2px solid #EEEEEE;
}
.item-left {
	width: 15vw;
	height: 60px;
	line-height: 60px;
	font-size: 15px;
	text-align: center;
}
.item-left text {
	background-color: #00E700;
	padding: 3px 9px;
	border-radius: 50px;
	color: #FFFFFF;
}
.item-middle {
	width: 70vw;
	height: 60px;
	line-height: 30px;
	font-size: 14px;
}
.item-right {
	height: 60px;
	line-height: 30px;
	text-align: right;
	padding-right: 5px;
	font-size: 14px;
}

