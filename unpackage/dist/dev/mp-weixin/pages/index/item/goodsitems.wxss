
.header {
	padding-top: 10rpx;
	padding-bottom: 10rpx;
	height: 60rpx;
}
.icon{
	width: 40rpx !important;
	height: 40rpx !important;
	float: left;
	margin-left: 10rpx;
}
.icon-img {
	width: 40rpx !important;
	height: 40rpx !important;
	margin-top: 10rpx;
}
.header-title {
	font-size: 40rpx;
	text-align: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.template-title {
	height: 24px;
	font-size: 36rpx;
	color: #1a1a1b;
	margin-left: 16rpx;
	padding-top: 14rpx;
	padding-bottom: 14rpx;
}
.template {
	border-top: 16rpx solid #EEEEEE;
}
.template .template-content{

width:100% ;
display: -webkit-box;
display: -webkit-flex;
display: flex;
-webkit-flex-wrap:wrap;
        flex-wrap:wrap;
border-top: .5px solid #8F8F94;
-webkit-box-pack: justify;
-webkit-justify-content: space-between;
        justify-content: space-between;
font-size: 34rpx;
}
.content-item{
margin-top: 18rpx;
width: 45%;
padding-left: 16rpx;
font-size: 32rpx;
}
.content-style-right {
	float: right;
	margin-right: 14rpx;
}

